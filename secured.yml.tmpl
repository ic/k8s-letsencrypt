apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: DEPLOYMENT_NAME-ingress
  labels:
    last_updated: "1000000000"
spec:
  tls:
  - hosts:
    - DOMAIN
    secretName: DEPLOYMENT_NAME-lscrypt-certs
  rules:
  - host: DOMAIN
    http:
      paths:
      - path: /*
        backend:
          serviceName: DEPLOYMENT_NAME-nginx
          servicePort: 80
